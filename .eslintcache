);\n        setProjectsInfo(data);\n        projectsInfo.map((data) => console.log(data.pictures.picture));\n      })\n      .catch((err) => {\n        console.log(\"error\");\n      });\n  }, []);\n\n  return (\n    <>\n      <h1>Projects:</h1>\n\n      {projectsInfo.map((data, index) => {\n        return (\n          <>\n            <h2>{data.name}</h2>\n            <h3>Description:</h3>\n            <p>{data.description}</p>\n            <a href={data.github_link}>link to Github repo</a>\n            <br />\n            <br />\n            <a href={data.deployed_link}>link to the website</a>\n            <img src={data.pictures.picture} />\n            <ul>\n              <h3>Languages:</h3>\n              {data.tech_lang.map((lang) => {\n                return <li>{`${lang.name}: ${lang.description}`}</li>;\n              })}\n            </ul>\n            <ul>\n              <h3>Clients:</h3>\n              {data.clients.map((client) => {\n                return (\n                  <li>{`${client.name}, ${client.industry}, ${client.description}`}</li>\n                );\n              })}\n            </ul>\n            <ul>\n              <h3>Colaborators:</h3>\n              {data.teamates.map((teamate) => {\n                return (\n                  <>\n                    <li>\n                      {`${teamate.name}:`}\n                      <a\n                        href={teamate.github_link}\n                      >{` link to ${teamate.name} Github repo`}</a>\n                    </li>\n                    <br />\n                  </>\n                );\n              })}\n            </ul>\n          </>\n        );\n      })}\n    </>\n  );\n}\n","/home/wild/programs/quests/sebasportfolio/src/pages/Home/Home.js",[],"/home/wild/programs/quests/sebasportfolio/src/pages/AboutThis/AboutThis.js",[],"/home/wild/programs/quests/sebasportfolio/src/pages/AboutMe/AboutMe.js",[],"/home/wild/programs/quests/sebasportfolio/src/hooks/fetch.js",[],"/home/wil